{
  "name": "api",
  "version": "1.0.0",
  "description": "A Nodejs Typescript API boilerplate ",
  "main": "index.js",
  "scripts": {
    "test": "\"\"",
    "start-r": "ts-node ./develop/server.ts",
    "dev": "ts-node ./develop/server.ts",
    "dev:watch": "nodemon",
    "start": "node ./server/server.js",
    "build": "tsc",
    "setup": "npm install knex -g",
    "db:migrate": "knex migrate:latest --knexfile ./knexfile.ts",
    "db:rollback": "knex migrate:rollback --knexfile ./knexfile.ts",
    "db:seed": "knex seed:run --knexfile ./knexfile.ts"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^8.1.0",
    "express": "^4.18.2",
    "knex": "^0.19.4",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "objection": "^1.6.9",
    "pg": "^7.12.1",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.7.7",
    "typedi": "^0.8.0"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.1",
    "@types/express": "^4.17.1",
    "@types/multer": "^1.3.9",
    "@types/node": "^12.7.5",
    "log4js": "^5.1.0",
    "nodemon": "^1.19.2",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3"
  },
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "develop"
    ],
    "exec": "npm run start-r",
    "ext": "ts"
  }
}
